{
  "kind": "collectionType",
  "collectionName": "subcategories",
  "info": {
    "singularName": "subcategory",
    "pluralName": "subcategories",
    "displayName": "Subcategory",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "slug": {
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "type": "uid",
      "regex": "^[a-z0-9]+(?:-[a-z0-9]+)*$"
    },
    "category": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::category.category",
      "inversedBy": "subcategories"
    },
    "products": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::product.product",
      "mappedBy": "subcategories"
    }
  }
}
