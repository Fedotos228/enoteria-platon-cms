{
  "kind": "collectionType",
  "collectionName": "news",
  "info": {
    "singularName": "new",
    "pluralName": "news",
    "displayName": "Articles",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "title": {
      "type": "text",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "body": {
      "type": "blocks",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "slug": {
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "type": "uid",
      "regex": "^[a-z0-9]+(?:-[a-z0-9]+)*$"
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": ["images"],
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "gallery": {
      "allowedTypes": ["images"],
      "type": "media",
      "multiple": true,
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "required": false
    }
  }
}
